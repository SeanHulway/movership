.ui.huge.header Have any questions?
%form.ui.form
  .field
    %label Name
    %input{name: 'name', placeholder: 'Name', type: 'text'}
  .field
    %label Email
    %input{name: 'email', placeholder: 'Email', type: 'text'}
  .field
    %label Phone Number
    %input{name: 'phone', placeholder: 'Phone Number', type: 'text'}
  .field
    %label Message
    %textarea{name: 'message', placeholder: 'Message', type: 'textarea', rows: '2'}
  .field
    .ui.checkbox
      %input{type: 'checkbox'}
      %label I agree to the Terms and Conditions
  %button.ui.button{type: 'submit'}
    Submit
